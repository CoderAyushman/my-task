{
  "expo": {
    "name": "My Tasks",
    "slug": "task-mate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/taskmate_splash1.png",
    "scheme": "taskmate",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/taskmate_splash1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ayushmanmishra.taskmate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/taskmate_splash1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/taskmate_splash1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4c44a97f-de1a-4409-991a-ba2a8c597015"
      }
    }
  }
}
